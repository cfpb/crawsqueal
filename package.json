{
  "name": "crawsqueal",
  "version": "1.0.0",
  "main": "index.js",
  "license": "CC0-1.0",
  "bin": "./index.js",
  "type": "module",
  "engines": {
    "node": ">=16.x"
  },
  "scripts": {
    "lint": "semistandard --fix",
    "fonts": "cp -r viewer/viewer/static_src/fonts viewer/viewer/static/fonts",
    "styles": "curl -o viewer/viewer/static/main.css https://www.consumerfinance.gov/static/css/main.css && curl https://www.consumerfinance.gov/static/apps/regulations3k/css/main.css >> viewer/viewer/static/main.css",
    "scripts": "esbuild viewer/viewer/static_src/crawsqueal.js --bundle --outfile=viewer/viewer/static/crawsqueal.js",
    "build": "yarn fonts && yarn styles && yarn scripts",
    "start": "node index.js"
  },
  "dependencies": {
    "@cfpb/cfpb-expandables": "^0.15.0",
    "ansi-colors": "^4.1.1",
    "cheerio": "^1.0.0-rc.10",
    "cli-progress": "^3.10.0",
    "esbuild": "^0.14.38",
    "html-minifier-terser": "^7.0.0-alpha.2",
    "md5": "^2.3.0",
    "simplecrawler": "^1.1.9",
    "sqlite": "^4.0.25",
    "sqlite3": "^5.0.2"
  },
  "devDependencies": {
    "semistandard": "^16.0.1"
  }
}
